<p id="notice"><%= notice %></p>

<h1>Listing Production Shifts</h1>

<table>
  <thead>
    <tr>
      <th>Material</th>
      <th>Shift</th>
      <th>Material quantity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @production_shifts.each do |production_shift| %>
      <tr>
        <td><%= production_shift.material %></td>
        <td><%= production_shift.shift %></td>
        <td><%= production_shift.material_quantity %></td>
        <td><%= link_to 'Show', production_shift %></td>
        <td><%= link_to 'Edit', edit_production_shift_path(production_shift) %></td>
        <td><%= link_to 'Destroy', production_shift, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Production shift', new_production_shift_path %>
