<p id="notice"><%= notice %></p>

<h1>Listing Employee Managers</h1>

<table>
  <thead>
    <tr>
      <th>Employee</th>
      <th>Crew</th>
      <th>Start date</th>
      <th>End date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_managers.each do |employee_manager| %>
      <tr>
        <td><%= employee_manager.employee %></td>
        <td><%= employee_manager.crew %></td>
        <td><%= employee_manager.start_date %></td>
        <td><%= employee_manager.end_date %></td>
        <td><%= link_to 'Show', employee_manager %></td>
        <td><%= link_to 'Edit', edit_employee_manager_path(employee_manager) %></td>
        <td><%= link_to 'Destroy', employee_manager, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employee manager', new_employee_manager_path %>
