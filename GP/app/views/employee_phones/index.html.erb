<p id="notice"><%= notice %></p>

<h1>Listing Employee Phones</h1>

<table>
  <thead>
    <tr>
      <th>Employee</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_phones.each do |employee_phone| %>
      <tr>
        <td><%= employee_phone.employee %></td>
        <td><%= employee_phone.phone %></td>
        <td><%= link_to 'Show', employee_phone %></td>
        <td><%= link_to 'Edit', edit_employee_phone_path(employee_phone) %></td>
        <td><%= link_to 'Destroy', employee_phone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employee phone', new_employee_phone_path %>
