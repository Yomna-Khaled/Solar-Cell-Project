
<h1>Listing Vendor Spares</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Vendor</th>
      <th>Spare part</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vendor_spares.each do |vendor_spare| %>
      <tr>
        <td><%= vendor_spare.date %></td>
        <td><%= vendor_spare.vendor %></td>
        <td><%= vendor_spare.spare_part %></td>
        <td><%= link_to 'Show', vendor_spare %></td>
        <td><%= link_to 'Edit', edit_vendor_spare_path(vendor_spare) %></td>
        <td><%= link_to 'Destroy', vendor_spare, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vendor spare', new_vendor_spare_path %>
