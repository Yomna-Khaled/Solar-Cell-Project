<p id="notice"><%= notice %></p>

<h1>Listing Materials</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Quantity</th>
      <th>Price</th>
      <th>Min number</th>
      <th>Stockno</th>
      <th>Shelfno</th>
      <th>Production date</th>
      <th>Expiration date</th>
      <th>Quantity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @materials.each do |material| %>
      <tr>
        <td><%= material.name %></td>
        <td><%= material.quantity %></td>
        <td><%= material.price %></td>
        <td><%= material.Min_Number %></td>
        <td><%= material.stockNo %></td>
        <td><%= material.shelfNo %></td>
        <td><%= material.production_date %></td>
        <td><%= material.expiration_date %></td>
        <td><%= material.quantity %></td>
        <td><%= link_to 'Show', material %></td>
        <td><%= link_to 'Edit', edit_material_path(material) %></td>
        <td><%= link_to 'Destroy', material, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Material', new_material_path %>
