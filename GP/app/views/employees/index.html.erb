<p id="notice"><%= notice %></p>

<h1>Listing Employees</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Salary</th>
      <th>Houre rate</th>
      <th>Education level</th>
      <th>Governamental id</th>
      <th>Position</th>
      <th>Type</th>
      <th>Crew</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employees.each do |employee| %>
      <tr>
        <td><%= employee.first_name %></td>
        <td><%= employee.last_name %></td>
        <td><%= employee.salary %></td>
        <td><%= employee.houre_rate %></td>
        <td><%= employee.education_level %></td>
        <td><%= employee.Governamental_ID %></td>
        <td><%= employee.position %></td>
        <td><%= employee.type %></td>
        <td><%= employee.crew %></td>
        <td><%= link_to 'Show', employee %></td>
        <td><%= link_to 'Edit', edit_employee_path(employee) %></td>
        <td><%= link_to 'Destroy', employee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employee', new_employee_path %>
