<p id="notice"><%= notice %></p>
<h1 align="center">
  <span class="page-header">Watt Price</span>
</h1>
<div class="container" style="width: 80;">
  <div class="table-responsive">
    <table class="table table-hover">
      <thead>
        <tr class="success">
          <th class="text-center">Value</th>
          <th class="text-center">Edit</th>
        </tr>
      </thead>

      <tbody>
        <% @lookup_prices.each do |lookup_price| %>
        <tr>
          <td class="text-center" ><%= lookup_price.value %></td>
          <td class="text-center" ><%= link_to 'Edit', edit_lookup_price_path(lookup_price),class: "btn btn-success" %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>



