<p id="notice"><%= notice %></p>

<h1>Listing Containers</h1>

<table>
  <thead>
    <tr>
      <th>Width</th>
      <th>Height</th>
      <th>Serialno</th>
      <th>Capacity</th>
      <th>Price</th>
      <th>Total power</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @containers.each do |container| %>
      <tr>
        <td><%= container.width %></td>
        <td><%= container.height %></td>
        <td><%= container.serialNo %></td>
        <td><%= container.capacity %></td>
        <td><%= container.price %></td>
        <td><%= container.total_power %></td>
        <td><%= link_to 'Show', container %></td>
        <td><%= link_to 'Edit', edit_container_path(container) %></td>
        <td><%= link_to 'Destroy', container, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Container', new_container_path %>
